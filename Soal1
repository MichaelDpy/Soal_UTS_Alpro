#include <iostream>
using namespace std;


int main() {
    char input[50];  
    cout << "Masukkan nomor kartu digital: ";
    cin >> input;

    int panjang = 0;
    bool hanyaAngka = true;
    char awal1 = '\0';
    char awal2 = '\0';

    while (input[panjang] != '\0') {
        if (panjang == 0) awal1 = input[0];
        if (panjang == 1) awal2 = input[1];

 if (input[panjang] < '0' || input[panjang] > '9') {
            hanyaAngka = false;
        }
        panjang++;
    }
